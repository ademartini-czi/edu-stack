{
  "name": "edu-stack-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "codegen": "graphql-codegen --config codegen.ts",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development tsx --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prod": "cross-env NODE_ENV=production node ./build/server.js",
    "routes": "remix routes",
    "start": "npm run dev",
    "storybook": "run-p dev:css storybook:start",
    "storybook:build": "npm run build:css && storybook build --output-dir ./storybook-static --webpack-stats-json",
    "storybook:start": "storybook dev -p 6006",
    "test": "jest",
    "test:storybook:axe": "run-s storybook:build test:storybook:axeOnly",
    "test:storybook:axeOnly": "axe-storybook --build-dir ./storybook-static",
    "types": "tsc --noEmit",
    "validate": "run-p test lint types"
  },
  "prettier": "@chanzuckerberg/prettier-config-edu",
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@chanzuckerberg/eds": "^7.2.1",
    "@remix-run/css-bundle": "^1.10.0",
    "@remix-run/express": "^1.11.0",
    "@remix-run/node": "^1.11.0",
    "@remix-run/react": "^1.11.0",
    "@remix-run/server-runtime": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "graphql-request": "^5.1.0",
    "isbot": "^3.6.3",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@chanzuckerberg/axe-storybook-testing": "^6.3.0",
    "@chanzuckerberg/eslint-config-edu-js": "^1.0.2",
    "@chanzuckerberg/eslint-config-edu-ts": "^1.0.5",
    "@chanzuckerberg/eslint-plugin-edu-react": "^1.1.8",
    "@chanzuckerberg/prettier-config-edu": "^1.0.2",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/client-preset": "1.2.6",
    "@remix-run/dev": "^1.11.0",
    "@remix-run/eslint-config": "^1.11.0",
    "@storybook/addon-a11y": "^7.0.0-beta.25",
    "@storybook/addon-actions": "^7.0.0-beta.25",
    "@storybook/addon-essentials": "^7.0.0-beta.25",
    "@storybook/addon-interactions": "^7.0.0-beta.25",
    "@storybook/react": "^7.0.0-beta.25",
    "@storybook/react-webpack5": "^7.0.0-beta.25",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/testing-react": "^2.0.0-next.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "cookie": "^0.5.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "msw": "^0.49.3",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "start-server-and-test": "^1.15.2",
    "storybook": "^7.0.0-beta.25",
    "tailwindcss": "^3.2.2",
    "ts-jest": "^29.0.5",
    "tsconfig-paths": "^4.1.2",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18"
  }
}
